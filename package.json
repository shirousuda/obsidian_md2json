{
  "name": "obsidian-md2json",
  "version": "1.0.0",
  "description": "Convert Obsidian markdown files to JSON format",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "markdown",
    "json",
    "converter"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.20.1",
    "obsidian": "latest",
    "ts-node": "^10.9.2",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  },
  "type": "module"
}
